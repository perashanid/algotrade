{
  "name": "algorithmic-trading-platform",
  "version": "1.0.0",
  "description": "Algorithmic trading platform with custom constraints and portfolio tracking",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "vercel-build": "npm run build:frontend",
    "start": "cd backend && npm start",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "migrate": "cd backend && npm run migrate",
    "seed:demo": "cd backend && npm run seed:demo",
    "seed:stock-groups": "cd backend && npm run seed:stock-groups",
    "test:db": "cd backend && npm run test:db"
  },
  "keywords": ["trading", "algorithmic", "portfolio", "stocks", "finnhub"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@vercel/node": "^3.0.0"
  },
  "dependencies": {
    "@types/node": "^20.8.10",
    "typescript": "^5.2.2",
    "@vercel/node": "^3.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "winston": "^3.11.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.7"
  }
}